<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>refistration</servlet-name>
        <servlet-class>servlets.RegistrationServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>refistration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ok</servlet-name>
        <servlet-class>OkServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ok</servlet-name>
        <url-pattern>/ok</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>servlets.MainPageServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/MainPage</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>servlets.TestServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
</web-app>